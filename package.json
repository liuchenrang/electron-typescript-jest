{
  "name": "ele",
  "version": "1.0.1",
  "main": "dist/main/main.js",
  "license": "MIT",
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8090/release/"
      }
    ],
    "icon": "src/assets/logo.png",
    "productName": "ele",
    "appId": "xxxxx",
    "copyright": "xxxx",
    "compression": "store",
    "directories": {
      "output": "release"
    },
    "asar": true,
    "files": [
      "dist/main",
      "dist/renderer",
      "node_modules/"
    ]
  },
  "scripts": {
    "watch:node": "webpack watch --config config/webpack.node.config.js",
    "watch:main": "webpack watch --config config/webpack.main.config.js",
    "start:electron": "supervisor -w ./dist -x electron dist/main/main.js",
    "start:main": "concurrently 'yarn watch:main' 'yarn start:electron'",
    "start:all": "concurrently  'yarn watch:main' 'yarn start:electron' 'yarn start:renderer'",
    "build:main": "webpack build --config config/webpack.main.prod.config.js",
    "build:renderer": "webpack build --config config/webpack.renderer.prod.config.js",
    "start:renderer": "webpack serve --config config/webpack.renderer.config.js",
    "rebuild": "electron-rebuild  ",
    "m": "electron-builder -m",
    "build": "yarn build:main && yarn build:renderer && electron-builder -m",
    "w:res": "yarn build:main && yarn build:renderer",
    "w:exe": "electron-builder -w",
    "w": "yarn w:res && yarn:exe",
    "test": "jest",
    "debug:run": "concurrently 'yarn start:renderer' 'yarn watch:main' 'yarn start:electron:debug:run'",
    "debug:stop": "concurrently 'yarn start:renderer' 'yarn watch:main' 'yarn start:electron:debug:stop'",
    "start:electron:debug:run": "supervisor -w ./dist -x electron  dist/main/main.js -- --inspect=5858",
    "start:electron:debug:stop": "supervisor -w ./dist -x electron  dist/main/main.js -- --inspect-brk=5858 "
  },
  "dependencies": {
    "antd": "^4.16.3",
    "async": "^3.2.0",
    "cheerio": "^1.0.0-rc.9",
    "dayjs": "^1.10.5",
    "electron-asar-hot-updater": "^0.1.3",
    "electron-updater": "^4.6.1",
    "golang-wasm": "^0.1.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "request": "^2.88.2",
    "sb-canvas": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/async": "^3.2.6",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "electron": "10.4.7",
    "electron-builder": "^22.13.1",
    "electron-rebuild": "^2.3.5",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.3.1",
    "less": "^4.1.1",
    "less-loader": "^9.1.0",
    "nan": "2.14.0",
    "node-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "supervisor": "^0.12.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.4.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "__npminstall_done": false
}
